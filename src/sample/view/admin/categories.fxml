<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="900.0" styleClass="principal-bg" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.admin.CategorieController">
    <children>
        <fx:include fx:id="navbar" source="sidebar.fxml" />
      <TextField fx:id="newCategorieField" layoutX="389.0" layoutY="221.0" prefHeight="40.0" prefWidth="179.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="200.0" layoutY="227.0" text="Categorie à ajouter">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button layoutX="568.0" layoutY="221.0" mnemonicParsing="false" onAction="#ajouterCategorie" prefHeight="40.0" prefWidth="130.0" style="-fx-background-color: #252525; -fx-border-color: none;" text="Ajouter" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="modifiedCategorieField" layoutX="389.0" layoutY="409.0" prefHeight="40.0" prefWidth="179.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="200.0" layoutY="357.0" text="Categorie à modifier">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button layoutX="568.0" layoutY="409.0" mnemonicParsing="false" onAction="#modifierCategorie" prefHeight="40.0" prefWidth="130.0" style="-fx-background-color: #252525; -fx-border-color: none;" text="Modifier" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="568.0" layoutY="520.0" mnemonicParsing="false" onAction="#supprimerCategorie" prefHeight="40.0" prefWidth="130.0" style="-fx-background-color: #252525; -fx-border-color: none;" text="Supprimer" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="modifiedCategorieCB" layoutX="389.0" layoutY="351.0" prefHeight="40.0" prefWidth="312.0" promptText="--SELECTONNER--" style="-fx-background-radius: 20;" />
      <ComboBox fx:id="deletedCategorieCB" layoutX="389.0" layoutY="520.0" prefHeight="40.0" prefWidth="179.0" promptText="--SELECTONNER--" style="-fx-background-radius: 20;" />
      <Label layoutX="200.0" layoutY="526.0" text="Categorie Supprimer">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label layoutX="325.0" layoutY="87.0" text="Gestion des catégories">
         <font>
            <Font name="Berlin Sans FB" size="34.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
